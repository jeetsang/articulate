{
  "name": "articulate",
  "version": "0.0.0",
  "description": "Smart conversational interfaces",
  "repository": {
    "type": "git",
    "url": "git://github.com/samtecspg/articulate.git"
  },
  "engines": {
    "npm": ">=5",
    "node": ">=10.5.0"
  },
  "author": "Samtec Smart Platform Group",
  "license": "Apache-2.0",
  "main": "src/index.js",
  "scripts": {
    "start": "nodemon server --exec babel-node",
    "build": "cross-env NODE_ENV=production env-cmd .env webpack --config internals/webpack/webpack.prod.babel.js --color -p --progress --hide-modules --display-optimization-bailout",
    "build:dll": "node ./internals/scripts/dependencies.js",
    "preinstall": "npm run npmcheckversion",
    "postinstall": "npm run build:dll",
    "npmcheckversion": "node ./internals/scripts/npmcheckversion.js"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/tkh44/hapi-webpack-example/issues"
  },
  "homepage": "https://github.com/tkh44/hapi-webpack-example#readme",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-stage-0": "^7.0.0",
    "add-asset-html-webpack-plugin": "^3.1.3",
    "babel-loader": "^8.0.5",
    "babel-plugin-dynamic-import-node": "2.2.0",
    "babel-plugin-react-intl": "3.0.1",
    "babel-plugin-react-transform": "3.0.0",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.2",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-inline-elements": "6.22.0",
    "babel-plugin-transform-react-jsx-source": "6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "circular-dependency-plugin": "^5.0.2",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "env-cmd": "^8.0.2",
    "eventsource-polyfill": "^0.9.6",
    "exports-loader": "^0.7.0",
    "file-loader": "^3.0.1",
    "glob": "^7.1.3",
    "hapi": "^18.1.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.6.0",
    "nodemon": "^1.18.10",
    "offline-plugin": "^5.0.6",
    "react-scripts": "2.1.5",
    "shelljs": "^0.8.3",
    "style-loader": "^0.23.1",
    "svg-url-loader": "^2.3.2",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dashboard": "^3.0.0",
    "webpack-dev-middleware": "^3.6.0",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-pwa-manifest": "^4.0.0"
  },
  "dependencies": {
    "material-colors": "^1.2.6",
    "react": "^16.8.3",
    "react-dom": "^16.8.3"
  },
  "nodemonConfig": {
    "verbose": true,
    "ignore": [
      "node_modules"
    ],
    "delay": "2500"
  },
  "dllPlugin": {
    "path": "node_modules/react-boilerplate-dlls",
    "exclude": [
      "chalk",
      "compression",
      "cross-env",
      "express",
      "ip",
      "minimist",
      "env-cmd"
    ],
    "include": [
      "core-js",
      "lodash",
      "eventsource-polyfill"
    ]
  }
}
